	<!---->
	<ThingDef ParentName="BuildingBase">
		<defName>PLA_ParkingLot</defName>
		<label>parking lot</label>
		<description>A place for parking the vehicle, the vehicle will return to the parking lot when undrafted.</description>
		<thingClass>PLA.Vehicle.ParkingLot</thingClass>
		<graphicData>
			<texPath>Building/ParkingLot</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<tickerType>Normal</tickerType>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<statBases>
			<WorkToBuild>0</WorkToBuild>
			<WorkTableWorkSpeedFactor>0.5</WorkTableWorkSpeedFactor>
		</statBases>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<useHitPoints>false</useHitPoints>
		<size>(1,1)</size>
		<designationCategory>PLA_DesignationCategory_Vehicle</designationCategory>
		<passability>Standable</passability>
		<!--hasInteractionCell>True</hasInteractionCell>
        <interactionCellOffset>(0,0,-1)</interactionCellOffset-->
		<uiIconScale>0.17</uiIconScale>
		<surfaceType>Item</surfaceType>
