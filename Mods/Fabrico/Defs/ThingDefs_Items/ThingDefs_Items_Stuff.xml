				<MaxHitPoints>0.65</MaxHitPoints>
				<Beauty>1</Beauty>
				<Flammability>1</Flammability>
				<WorkToMake>0.7</WorkToMake>
				<WorkToBuild>0.7</WorkToBuild>
				<DoorOpenSpeed>1.2</DoorOpenSpeed>
			</statFactors>
			<canSuggestUseDefaultStuff>true</canSuggestUseDefaultStuff>
		</stuffProps>
		<isTechHediff>true</isTechHediff>
		<thingCategories>
			<li>ResourcesRaw</li>
		</thingCategories>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
		<weaponClasses>
			<li>Melee</li>
			<li>MeleeBlunt</li>
			<li>Neolithic</li>
		</weaponClasses>
		<tools>
			<li>
				<label>log</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>10</power>
				<cooldownTime>2</cooldownTime>
			</li>
		</tools>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<allowedArchonexusCount>150</allowedArchonexusCount>
