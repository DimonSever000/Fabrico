		<description>Smelt glass.</description>
		<jobString>Smelting glass.</jobString>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>5000</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Sand</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Sand</li>
			</thingDefs>
		</fixedIngredientFilter>
