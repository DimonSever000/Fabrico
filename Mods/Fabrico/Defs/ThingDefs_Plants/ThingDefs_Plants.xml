<Defs>
	<!---->
	<ThingDef ParentName="PlantBase">
		<defName>Plant_Onions</defName>
		<label>onion plant</label>
		<description>Stringy green stalks which give way to a bulbous, fragrant head contained in a papery skin. Onion is a versatile ingredient, making for a good base in vegetable dishes as well as a delicious accompaniment to meat-based meals.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.03</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plants/OnionPlant/Mature</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.5</fertilityMin>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawOnions</harvestedThingDef>
			<harvestYield>8</harvestYield>
			<sowResearchPrerequisites>
				<li>Vegetables</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponics</li>
			</sowTags>
			<topWindExposure>0.20</topWindExposure>
			<growDays>4</growDays>
			<fertilitySensitivity>1</fertilitySensitivity>
			<visualSizeRange>0.5~1</visualSizeRange>
			<immatureGraphicPath>Things/Plants/OnionPlant/Immature</immatureGraphicPath>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<sowMinSkill>4</sowMinSkill>
			<wildClusterRadius>2</wildClusterRadius>
			<wildClusterWeight>100</wildClusterWeight>
		</plant>
	</ThingDef>
