			<Mass>60</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>0</Beauty>
			<JoyGainFactor>1.3</JoyGainFactor>
		</statBases>
		<size>(2,3)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>100</costStuffCount>
		<costList>
			<Cloth>120</Cloth>
			<WoodLog>30</WoodLog>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<pathCost>42</pathCost>
		<passability>PassThroughOnly</passability>
