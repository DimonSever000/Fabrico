					<li>(0,0.25)</li>
					<li>(1,0)</li>
				</points>
			</speedCurve>
			<speed>0.5</speed>
			<explosionRadius>0.025</explosionRadius>
			<explosionDamage>Lands_Carryall</explosionDamage>
			<explosionDamageFactor>0.6</explosionDamageFactor>
		</skyfaller>
	</ThingDef>
