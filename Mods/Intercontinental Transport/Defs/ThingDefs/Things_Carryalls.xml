<Defs>
	<!---->
	<ThingDef Name="CarryallSkyfallerBase" ParentName="SkyfallerBase" Abstract="True">
		<size>(1,1)</size>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutFlying</shaderType>
		</graphicData>
		<skyfaller>
			<movementType>Decelerate</movementType>
			<shadow>Things/Skyfaller/SkyfallerShadowRectangle</shadow>
			<motesPerCell>1</motesPerCell>
		</skyfaller>
	</ThingDef>
	<!---->
	<ThingDef ParentName="CarryallSkyfallerBase">
		<defName>Airship_Kirov_Leaving</defName>
		<label>kirov airship (leaving)</label>
		<thingClass>SRTS.SRTSLeaving</thingClass>
		<graphicData>
			<texPath>Carryall/AirshipKirov</texPath>
			<drawSize>(6,6)</drawSize>
		</graphicData>
		<skyfaller>
			<shadowSize>(4,2)</shadowSize>
			<reversed>true</reversed>
			<anticipationSound>Carryall_Leaving</anticipationSound>
			<anticipationSoundTicks>-20</anticipationSoundTicks>
			<ticksToImpactRange>-15~-10</ticksToImpactRange>
			<moteSpawnTime>0.05</moteSpawnTime>
			<angleCurve>
				<points>
					<li>(0,90)</li>
					<li>(0.2,90)</li>
					<li>(1,60)</li>
				</points>
			</angleCurve>
			<rotationCurve>
				<points>
					<li>(0,90)</li>
					<li>(0.2,90)</li>
					<li>(1,80)</li>
				</points>
			</rotationCurve>
			<zPositionCurve>
				<points>
					<li>(0,0)</li>
					<li>(0.2,2)</li>
				</points>
			</zPositionCurve>
			<speedCurve>
				<points>
					<li>(0,0)</li>
					<li>(0.2,0.02)</li>
					<li>(1,0.25)</li>
				</points>
			</speedCurve>
			<speed>0.5</speed>
		</skyfaller>
	</ThingDef>
